{
  "name": "@battis/cli",
  "version": "0.1.5-2",
  "description": "Quickly build a CLI app",
  "author": {
    "name": "Seth Battis",
    "url": "https://github.com/battis"
  },
  "license": "GPL-3.0",
  "main": "lib/index.js",
